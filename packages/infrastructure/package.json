{
  "name": "@q-view/infrastructure",
  "version": "0.1.0",
  "private": true,
  "description": "AWS CDK infrastructure for Q Conversation Viewer",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "deploy:infra:ci": "cdk deploy --require-approval never",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "destroy": "cdk destroy",
    "bootstrap": "cdk bootstrap",
    "clean": "rm -rf node_modules cdk.out *.js *.d.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "20.x",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "aws-cdk": "2.167.1",
    "typescript": "~5.0.0",
    "source-map-support": "^0.5.21"
  },
  "dependencies": {
    "aws-cdk-lib": "2.167.1",
    "constructs": "^10.0.0"
  }
}
